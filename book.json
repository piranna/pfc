{
  "gitbook": "2.6.7",
  "plugins":
  [
    "anchors",
    "autocover",
    "image-captions",
    "indexes",
    "mermaid",
    "printlinks"
  ],
  "pluginsConfig": {
    "autocover": {
      "title": "NodeOS",
      "author": "Jesús Leganés Combarro",
      "tutor": "Enrique Soriano Salvador",
      "academicYear": "2015/2016",
      "font": {
        "family": "Impact",
        "color": "#FFF"
      },
      "background": {
        "color": "#09F"
      },
      "template": "cover.svg"
    },
    "image-captions": {
      "caption": "_CAPTION_"
    },
    "printlinks": {
      "externalLinksAreReferences": true
    }
  },
  "pdf": {
    "fontSize": 16,
    "pageNumbers": true
  }
}
